# Aurora Configuration
# Copy this file to .env and fill in your actual values

# ClickUp Configuration
# Get your token from: https://app.clickup.com/settings/apps
CLICKUP_TOKEN=your_clickup_api_token_here
CLICKUP_WORKSPACE_ID=your_workspace_id_here
CLICKUP_SPACE_ID=your_space_id_here_optional

# TickTick Configuration
# TickTick requires an API token from their developer portal
# Leave empty to skip TickTick integration
TICKTICK_USERNAME=your_ticktick_api_token_here
TICKTICK_PASSWORD=not_used_with_api_token

# Resend Email Configuration
# Get your API key from: https://resend.com/api-keys
# Use a verified domain for FROM_EMAIL
RESEND_API_KEY=re_your_resend_api_key_here
RESEND_FROM_EMAIL=aurora@mail.yourdomain.com
RESEND_TO_EMAIL=your_email@example.com

gemini
GEMINI_API_KEY=your_gemini_token

# Application Configuration
TIMEZONE=America/Santiago
LOG_LEVEL=INFO
